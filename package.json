{
  "name": "pcweb",
  "version": "1.1.0",
  "description": "VD project boilerplate",
  "main": "index.js",
  "scripts": {
    "dll": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.dll.js",
    "dev:dll": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.dll.js",
    "prod:dll": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.dll.js",
    "start": "node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.dev.js  --open --progress --profile --watch --content-base src/",
    "release": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.prod.js --progress",
    "deploy": "npm run release && deploy-client",
    "test": "jest"
  },
  "keywords": [
    "Webpack",
    "Babel",
    "Dva",
    "React",
    "React Router",
    "Redux",
    "Redux Saga",
    "PostCSS",
    "Less",
    "Axios",
    "Mock.js",
    "Jest",
    "Enzyme",
    "Ant Design"
  ],
